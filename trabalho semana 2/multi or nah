#include <stdio.h>
int main () {
//Declaracao de variaveis
  int num1, numo2;

//Entrada de Dados
  printf ("Informe o primeiro numero para calcular: ");
  scanf ("%d", &num1);
  printf ("Informe o segundo numero para calcular: ");
  scanf ("%d", &num2);
//Processamento
  if (num1 % num2==0){
    printf ("Os números são múltiplos ");
  } else{
    printf ("Os números não são múltiplos ");
  }

return 0;
}
