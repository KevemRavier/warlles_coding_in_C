#include <stdio.h>

int main() {
   int dig1, dig2, dig3, dig4, dig5, numero;

//entrada de dados
   printf("escreva um numero de 5 digitos: ");
   scanf("%d", &numero);

   dig1 = numero / 10000;
   dig2 = (numero / 1000) % 10;
   dig3 = (numero / 100) % 10;
   dig4 = (numero / 10) % 10;
   dig5 = numero % 10;
//saida de dados
   printf("%d   %d   %d   %d   %d\n", dig1, dig2, dig3, dig4, dig5);

   return 0;
}
